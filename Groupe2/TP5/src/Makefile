CC ?= gcc

CFLAGS=-Wall -Wextra -g

COBJS ?= client.o operator.o combinator.o
SOBJS ?= serveur.o operator.o combinator.o

.SUFFIXES: .c .o

SERVER = serveur
CLIENT = client

all: $(SERVER) $(CLIENT)

$(SERVER): $(SOBJS)
	$(CC) -o $(SERVER) $(SOBJS)

$(CLIENT): $(COBJS)
	$(CC) -o $(CLIENT) $(COBJS)

.c.o: 
	$(CC) -c $*.c

